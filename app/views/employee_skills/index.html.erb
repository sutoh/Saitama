<div class="page-header">
  <h1>Listing employee_skills</h1>
</div>
<h2><%= @employee.fullname %></h2>

<div class="well">
  <div class="table">
    <div class="skills_header_row">
      <div class="skills_col">Skill</div>
      <div class="skills_col">Level</div>
    </div>
    <% @have_employee_skills.each do | employee_skill| %>
      <div id="employee_skill_<%= employee_skill.skill_id %>">
        <%= render employee_skill %>
      </div>
    <% end %>
    <% @have_not_employee_skills.each do | employee_skill| %>
      <div id="employee_skill_<%= employee_skill.skill_id %>">
        <%= render employee_skill %>
      </div>
    <% end %>
    <div class="skills_row"></div>
  </div>
</div>
<%= link_to 'New Employee skill', new_employee_employee_skill_path(params[:employee_id]), :class => "btn btn-success" %>
<%= link_to 'Back', employees_path, :class => "btn" %>